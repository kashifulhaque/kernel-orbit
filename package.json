{
  "name": "kernel-orbit",
  "displayName": "Kernel Orbit",
  "description": "Kernel Orbit lets you run CUDA/Triton kernels and Jupyter notebooks on modal.com GPUs with benchmarking and profiling",
  "version": "0.2.6",
  "publisher": "ifkash",
  "repository": {
    "type": "git",
    "url": "https://github.com/kashifulhaque/kernel-orbit"
  },
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Programming Languages"
  ],
  "keywords": [
    "cuda",
    "triton",
    "gpu",
    "modal",
    "kernel",
    "profiling",
    "benchmark",
    "jupyter",
    "notebook",
    "remote execution"
  ],
  "activationEvents": [
    "onLanguage:cuda-cpp",
    "onLanguage:cpp",
    "onLanguage:python",
    "onNotebook:jupyter-notebook",
    "workspaceContains:**/*.cu",
    "workspaceContains:**/*.cuh",
    "workspaceContains:**/*.ipynb"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "modalKernel.runKernel",
        "title": "Run Kernel on Modal",
        "category": "Kernel Orbit",
        "icon": "$(play)"
      },
      {
        "command": "modalKernel.selectGpu",
        "title": "Select GPU Type",
        "category": "Kernel Orbit",
        "icon": "$(server)"
      },
      {
        "command": "modalKernel.showResults",
        "title": "Show Results Panel",
        "category": "Kernel Orbit",
        "icon": "$(graph)"
      },
      {
        "command": "modalKernel.exportResults",
        "title": "Export Results",
        "category": "Kernel Orbit",
        "icon": "$(export)"
      },
      {
        "command": "modalKernel.setupModal",
        "title": "Setup Modal Environment",
        "category": "Kernel Orbit",
        "icon": "$(gear)"
      },
      {
        "command": "modalKernel.checkStatus",
        "title": "Check Modal Status",
        "category": "Kernel Orbit",
        "icon": "$(info)"
      },
      {
        "command": "modalKernel.reloadCredentials",
        "title": "Reload Credentials",
        "category": "Kernel Orbit",
        "icon": "$(refresh)"
      },
      {
        "command": "modalKernel.warmupImages",
        "title": "Warmup Images (Pre-build for faster runs)",
        "category": "Kernel Orbit",
        "icon": "$(flame)"
      },
      {
        "command": "modalKernel.restartNotebookKernel",
        "title": "Restart Modal GPU Kernel",
        "category": "Kernel Orbit",
        "icon": "$(refresh)"
      },
      {
        "command": "modalKernel.terminateNotebookSession",
        "title": "Terminate Modal GPU Session",
        "category": "Kernel Orbit",
        "icon": "$(stop)"
      },
      {
        "command": "modalKernel.killSession",
        "title": "Kill Session",
        "category": "Kernel Orbit",
        "icon": "$(close)"
      },
      {
        "command": "modalKernel.selectGpuFromTree",
        "title": "Select GPU",
        "category": "Kernel Orbit"
      },
      {
        "command": "modalKernel.profileKernel",
        "title": "Profile Kernel on GPU",
        "category": "Kernel Orbit",
        "icon": "$(flame)"
      },
      {
        "command": "modalKernel.showProfiling",
        "title": "Show Profiling Results",
        "category": "Kernel Orbit",
        "icon": "$(dashboard)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "modalKernel.selectGpuFromTree",
          "when": "false"
        }
      ],
      "editor/title": [
        {
          "command": "modalKernel.runKernel",
          "when": "resourceExtname == .cu || resourceExtname == .py",
          "group": "navigation"
        },
        {
          "command": "modalKernel.profileKernel",
          "when": "resourceExtname == .cu || resourceExtname == .py",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "modalKernel.runKernel",
          "when": "resourceExtname == .cu || resourceExtname == .py",
          "group": "1_modal"
        },
        {
          "command": "modalKernel.profileKernel",
          "when": "resourceExtname == .cu || resourceExtname == .py",
          "group": "1_modal"
        },
        {
          "command": "modalKernel.selectGpu",
          "when": "resourceExtname == .cu || resourceExtname == .py",
          "group": "1_modal"
        }
      ],
      "view/item/context": [
        {
          "command": "modalKernel.killSession",
          "when": "view == modalKernel.sessions && viewItem == activeSession",
          "group": "inline"
        }
      ]
    },
    "keybindings": [
      {
        "command": "modalKernel.runKernel",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus && (resourceExtname == .cu || resourceExtname == .py)"
      },
      {
        "command": "modalKernel.profileKernel",
        "key": "ctrl+shift+p",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus && (resourceExtname == .cu || resourceExtname == .py)"
      }
    ],
    "configuration": {
      "title": "Kernel Orbit",
      "properties": {
        "modalKernel.defaultGpu": {
          "type": "string",
          "default": "T4",
          "enum": [
            "T4",
            "L4",
            "A10G",
            "A100-40GB",
            "A100-80GB",
            "L40S",
            "H100",
            "H200",
            "B200"
          ],
          "description": "Default GPU type to use for kernel execution"
        },
        "modalKernel.gpuCount": {
          "type": "number",
          "default": 1,
          "minimum": 1,
          "maximum": 8,
          "description": "Number of GPUs to attach to the container"
        },
        "modalKernel.timeout": {
          "type": "number",
          "default": 3600,
          "minimum": 10,
          "maximum": 86400,
          "description": "Execution timeout in seconds (max time a single notebook cell can run)"
        },
        "modalKernel.warmupRuns": {
          "type": "number",
          "default": 3,
          "minimum": 0,
          "maximum": 100,
          "description": "Number of warmup runs before benchmarking"
        },
        "modalKernel.benchmarkRuns": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "maximum": 1000,
          "description": "Number of benchmark runs for timing"
        },
        "modalKernel.enableProfiling": {
          "type": "boolean",
          "default": true,
          "description": "Enable CUDA profiling (nsys/ncu style metrics)"
        },
        "modalKernel.autoSave": {
          "type": "boolean",
          "default": true,
          "description": "Automatically save file before running"
        },
        "modalKernel.profilingMode": {
          "type": "string",
          "default": "summary",
          "enum": ["summary", "full"],
          "enumDescriptions": [
            "Collect basic profiling metrics (SM efficiency, memory throughput, occupancy, execution time)",
            "Collect full profiling metrics including roofline and cache data (slower)"
          ],
          "description": "Nsight Compute profiling data collection level"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "modalKernelExplorer",
          "title": "Kernel Orbit",
          "icon": "$(server)"
        }
      ]
    },
    "views": {
      "modalKernelExplorer": [
        {
          "id": "modalKernel.gpuPicker",
          "name": "GPU Selection"
        },
        {
          "id": "modalKernel.sessions",
          "name": "Active Sessions"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}